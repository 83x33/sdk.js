{
  "name": "sdk33",
  "version": "1.0.0",
  "description": "8333.io javascript software development kit.",
  "source": "src/index.js",
  "main": "dist/index.js",
  "scripts": {
    "build": "    microbundle --name sdk33 --format cjs,umd --sourcemap false --external all",
