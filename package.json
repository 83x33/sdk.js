  },
  "files": [
    "index.js",
    "dist/index.js",
    "dist/index.umd.js"
  ],
  "devDependencies": {
    "microbundle": "^0.7.0"
  },
  "dependencies": {
    "axios": "^0.18.0"
  }
}
